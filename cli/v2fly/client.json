{
  "log": {
    "loglevel": "none"
  },
  "inbounds": [
    {
      "port": <port>,
      "listen": "127.0.0.1",
      "protocol": "http"
    }
  ],
  "outbounds": [
    {
      "protocol": "vmess",
      "settings": {
        "vnext": [
          {
            "address": "<vmess-address>",
            "port": <vmess-port>,
            "users": [
              {
                "id": "<vmess-id>",
                "aid":<vmess-aid>,
                "alterId": <vmess-alterId>,
                "security": "<vmess-security>"
              }
            ]
          }
        ]
      },
      "streamSettings": {
        "network": "<streamSettings-network>",
        "grpcSettings": {
          "serviceName": <streamSettings-grpcSettings-serviceName>
        },
        "security": "<streamSettings-security>",
        "tlsSettings": {
          "serverName": <streamSettings-tlsSettings-serverName>
        },
        "wsSettings": null
      }
    },
    {
      "protocol": "freedom",
      "settings": {},
      "tag": "direct"
    }
  ],
  "routing": {
    "domainStrategy": "IPIfNonMatch",
    "rules": [
      {
        "type": "field",
        "domains": [
          "localhost"
        ],
        "ip": [
          "127.0.0.1"
        ],
        "outboundTag": "direct"
      }
    ]
  }
}